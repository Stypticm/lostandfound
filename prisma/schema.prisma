generator client {
  provider = "prisma-client-js"
  output = "../src/lib/db/generated"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Item {
  id          Int      @id @default(autoincrement())
  title       String
  type        String
  city        String
  description String
  imageUrl    String?
  telegramId  String?
  createdAt   DateTime @default(now())
}
