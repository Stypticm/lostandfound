generator client {
  provider      = "prisma-client-js"
  output        = "../../node_modules/generated"
  engineType    = "binary"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Item {
  id          Int      @id @default(autoincrement())
  title       String
  type        String
  city        String
  description String
  imageUrl    String?
  telegramId  String?
  createdAt   DateTime @default(now())
}
